{
   "name": "vscode-scheme",
   "displayName": "vscode-scheme",
   "description": "Scheme language support for VSCode",
   "keywords": [
      "scheme",
      "syntax"
   ],
   "version": "0.5.0",
   "publisher": "Knights879",
   "icon": "images/lambda.png",
   "main": "./out/src/schemeMain",
   "license": "MIT",
   "author": {
      "name": "Allen Huang & Ethan Hardy & Austin Traub"
   },
   "engines": {
      "vscode": "^1.11.0"
   },
   "categories": [
      "Programming Languages",
      "Snippets"
   ],
   "activationEvents": [],
   "repository": {
      "type": "git",
      "url": "https://github.com/Knights879/vscode-scheme.git"
   },
   "bugs": {
      "url": "https://github.com/Knights879/vscode-scheme/issues"
   },
   "homepage": "https://github.com/Knights879/vscode-scheme",
   "contributes": {
      "languages": [
         {
            "id": "scheme",
            "aliases": [
               "Scheme",
               "scheme"
            ],
            "extensions": [
               ".scm",
               ".ss",
               ".sch",
               ".rkt"
            ],
            "configuration": "./scheme.configuration.json"
         }
      ],
      "grammars": [
         {
            "language": "scheme",
            "scopeName": "source.scheme",
            "path": "./syntaxes/scheme.tmLanguage"
         },
         {
            "scopeName": "markdown.scheme.codeblock",
            "path": "./syntaxes/scheme.markdown.tmLanguage.json",
            "injectTo": [
               "text.html.markdown"
            ]
         }
      ],
      "snippets": [
         {
            "language": "scheme",
            "path": "./snippets/scheme.json"
         }
      ]
   },
   "scripts": {
      "vscode:prepublish": "tsc -p ./",
      "compile": "tsc -watch -p ./",
      "test": "vscode-test-electron"
   },
   "devDependencies": {
      "@types/mocha": "^10.0.10",
      "@types/node": "^22.13.1",
      "@types/vscode": "^1.103.0",
      "@vscode/test-electron": "^2.5.2",
      "mocha": "^11.1.0",
      "typescript": "^5.7.3"
   }
}
